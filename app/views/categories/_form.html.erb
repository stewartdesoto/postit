<%= render 'shared/error_messages', obj:@category %>

<div class="well"> 
  <%= form_for @category do |f| %>
    <div class="control-group">
      <%= f.label :name %>
      <%= f.text_field :name %>
    </div>
    <div class="control-group">
      <%= f.submit "Create",  class:"btn btn-primary" %>
    </div>
  <% end %>
</div>